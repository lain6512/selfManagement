.container{
    background: #ffffff;
}
.dynamic-list{
    font-size: 12px;
    border-bottom:#eee 1rpx solid;
    display: flex;
    flex-flow:row;
}
.dynamic-list .head {
    width: 50px;
    text-align: center;
}

.dynamic-list .head .head-map{
    width: 32px;
    height: 32px;
    border-radius: 100px;
    border:#eee 1px solid;
    margin:0 auto;
}
.dynamic-list .head .head-map .img{
    width: 31px;
    height: 31px;
    border-radius: 100px;

}
.dynamic-list .info {
    flex:1;
    position: relative;
}

.dynamic-list .info .ico-tips {
    position: absolute;
    top: 10px;
    left: -5px;
    z-index: 10;
    width: 0;
    height: 0;
    border-top: 4px solid transparent;
    border-bottom: 4px solid transparent;
    border-right: 6px solid #ccc;
    opacity: 1;
}
.dynamic-list .info .ico-tips2 {
    position: absolute;
    top: 12px;
    left: -3px;
    z-index: 11;
    width: 0;
    height: 0;
    border-top: 2px solid transparent;
    border-bottom: 2px solid transparent;
    border-right: 4px solid #ffffff;
    opacity: 1;
}
.dynamic-list .info .message {
    border:#d1d1d1 1px solid;
    padding: 5px 10px;
    /*color: #0bb20c;*/
    font-size: 13px;
    margin-right: 5px;
    border-radius: 5px;
}
.dynamic-list .info .info-inner{
    display: flex;
    flex-flow:row;
}
.dynamic-list .info .info-inner .left{
    padding-left:7px;
    flex:1;
}
.dynamic-list .info .info-inner .left .txt{
    font-size: 12px;
}

.dynamic-list .info .info-inner .show{
    padding:0px 10px 0 0 ;
    width:100px;
}

.head .name{
    font-size:10px;
}
.dynamic-list .center{
    padding-left: 15px;
}
.dynamic-list .num{
    font-size: 14px;
}

.panel-tips-css{
    position: absolute;
    top:8px;
    left: -6px;
    z-index: 50;
    width: 7px;
    height: 9px;
    background: url("data:image/png;base64,") no-repeat;
}